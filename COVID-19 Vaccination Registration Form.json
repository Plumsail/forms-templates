	"js": "fd.rendered(function() {\r\n\r\n    //disable VaccinationDate date fields\r\n    fd.field('VaccinationDate').disabled = true;\r\n    //set VaccinationDate to the current date\r\n    fd.field('VaccinationDate').value = new Date();\r\n\r\n    //load the Moment.js library to disable dates\r\n    $.getScript('https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.24.0/moment-with-locales.min.js')\r\n        .then(function() {\r\n            birthDateCalendarSetUp();\r\n\r\n        });\r\n        \r\n    //set up calendar start view and disable dates\r\n    fd.field('DiagnosedCOVIDDate').widgetOptions = {\r\n        max: new Date(),\r\n        min: new Date('2020-03-01'),\r\n        start: 'decade'\r\n    }\r\n    \r\n    fd.field('ImmunizationDate').widgetOptions = {\r\n        max: new Date(),\r\n        min: new Date('2021-01-01'),\r\n        start: 'decade'\r\n    }\r\n\r\n    //call functions when changing field values    \r\n    fd.field('ChronicConditions').$on('change', showHideDiagnosedChronicHealthProblems);\r\n    fd.field('DiagnosedCOVID').$on('change', showHideDiagnosedCOVIDDate);\r\n    fd.field('Immunized').$on('change', showHideImmunizationDate);\r\n\r\n    //сalling functions on form loading\r\n    showHideDiagnosedChronicHealthProblems();\r\n    showHideDiagnosedCOVIDDate();\r\n    showHideImmunizationDate();\r\n\r\n});\r\n\r\n//disable dates to allow only 18+ users\r\n//set up calendar start view\r\nfunction birthDateCalendarSetUp() {\r\n    var today = moment(new Date());\r\n    var newDate = today.subtract(18, 'years');\r\n    fd.field('BirthDate').widgetOptions = {\r\n        max: moment(newDate).toDate(),\r\n        start: 'decade'\r\n    }\r\n}\r\n\r\n//show hide DiagnosedChronicHealthProblems field\r\nfunction showHideDiagnosedChronicHealthProblems() {\r\n    if (fd.field('ChronicConditions').value == 'Yes') {\r\n        fd.field('DiagnosedChronicHealthProblems').hidden = false;\r\n    } else {\r\n        fd.field('DiagnosedChronicHealthProblems').hidden = true;\r\n    }\r\n}\r\n\r\n//show hide DiagnosedCOVIDDate field\r\nfunction showHideDiagnosedCOVIDDate() {\r\n    if (fd.field('DiagnosedCOVID').value == 'Yes') {\r\n        fd.field('DiagnosedCOVIDDate').hidden = false;\r\n    } else {\r\n        fd.field('DiagnosedCOVIDDate').hidden = true;\r\n    }\r\n}\r\n\r\n//show hide ImmunizationDate field\r\nfunction showHideImmunizationDate() {\r\n    if (fd.field('Immunized').value == 'Yes') {\r\n        fd.field('ImmunizationDate').hidden = false;\r\n    } else {\r\n        fd.field('ImmunizationDate').hidden = true;\r\n    }\r\n}",
	"css": "/** styling of the disable Date field **/\n\n.fd-form .vaccination-date .form-control:disabled,\n.fd-form .vaccination-date .form-control[readonly],\n.fd-form .vaccination-date .k-datepicker .k-picker-wrap,\n.fd-form .vaccination-date  .form-control{\n    background-color: transparent;\n    border-style: none;\n    font-weight: bold;\n}\n\n.fd-form .vaccination-date .k-select {\n    display: none !important;\n}\n\n.fd-form .vaccination-date input {\n    padding-left: 0px;\n}\n",
