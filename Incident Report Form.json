  "js": "// function that shows/hides Other field\r\nfunction toggleOtherType() {\r\n    if (fd.field('IncidentType').value.includes('Other')) {\r\n        fd.field('OtherIncidentType').hidden = false;\r\n    } else {\r\n        fd.field('OtherIncidentType').hidden = true;\r\n    }\r\n}\r\n\r\nfd.rendered(function () {\r\n    // call function on form load\r\n    toggleOtherType();\r\n\r\n    // call function when the field value is changed\r\n    fd.field('IncidentType').$on('change', toggleOtherType);\r\n});\r\n",
  "css": "@media (min-width: 768px) {\n    .fd-form .col-md-auto {\n        width: auto !important;\n        flex: 0 0 auto !important;\n    }\n}\n\n.fd-form h1, .fd-form .h1 {\n    font-size: min(6.5vw, 2.34375rem) !important;\n}\n\n.fd-form .btn-primary {\n    font-weight: 500 !important;\n    color: white !important;\n}\n\n.fd-form .fd-total-field span.k-select {\n    display: none !important;\n}\n\n.fd-form .fd-payment-form {\n    display: none;\n}\n",
