  "js": "function toggleOtherOrgTypeField() {\n    if (fd.field('OrgType').value === 'Other (see instructions)') {\n        fd.field('OtherOrgType').hidden = false;\n    } else {\n        fd.field('OtherOrgType').hidden = true;\n    }\n}\n\nfunction toggleLlcTaxClassificationField() {\n    if (fd.field('OrgType').value === 'Limited liability company') {\n        $('.fd-llc').show();\n    } else {\n        $('.fd-llc').hide();\n    }\n}\n\nfd.rendered(function() {\n    toggleOtherOrgTypeField();\n    toggleLlcTaxClassificationField();\n    \n    fd.field('OrgType').$on('change', function() {\n        toggleOtherOrgTypeField();\n        toggleLlcTaxClassificationField();\n    })\n})",
  "css": ".fd-form h1, .fd-form .h1 {\r\n    font-size: min(6.5vw, 2.34375rem) !important;\r\n    text-align: left !important;\r\n}\r\n\r\n.fd-form h1:after {\r\n    content: ' ';\r\n    display: block;\r\n    border-top: 3px solid;\r\n    padding-bottom: 10px;\r\n    margin-top: 10px;\r\n}\r\n\r\n.fd-form h4, .fd-form .h4 {\r\n    font-size: min(4.5vw, 1.40625rem) !important;\r\n    padding-top: 4px !important;\r\n    padding-bottom: 4px !important;\r\n}\r\n\r\n",
