                              "required": false,
                              "font": {
                                "style": "Normal",
                                "weight": "Semibold"
                              },
                              "cFont": {
                                "style": "Normal",
                                "weight": "Normal"
                              },
                              "mask": "00-0000000",
                              "wrap": true,
