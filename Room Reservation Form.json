    "js": "// show/hide ArrivalTime and FlightNumber fields and make them mandatory when PickUp field is enabled\r\nfunction toggleArrivalInfo() {\r\n    var pickUpRequired = fd.field('PickUp').value\r\n    \r\n    if(pickUpRequired) {\r\n        fd.field('ArrivalTime').required = true;\r\n        fd.field('FlightNumber').required = true;\r\n        \r\n        fd.field('ArrivalTime').hidden = false\r\n        fd.field('FlightNumber').hidden = false;\r\n    }\r\n    else {\r\n        fd.field('ArrivalTime').required = false;\r\n        fd.field('FlightNumber').required = false;\r\n        \r\n        fd.field('ArrivalTime').hidden = true;\r\n        fd.field('FlightNumber').hidden = true;\r\n    }\r\n}\r\n\r\nfd.rendered(function () {\r\n    \r\n    // validation of the time interval\r\n    fd.validators.push({\r\n        name: 'Start and End time validation',\r\n        error: 'Please select valid start and end time',\r\n        validate: function(value) {\r\n            if (fd.field('CheckInDate').value > fd.field('CheckOutDate').value)\r\n                return false;\r\n    \r\n            return true;\r\n        }\r\n    });\r\n    \r\n    \r\n    // call the function on form load\r\n    toggleArrivalInfo();\r\n    \r\n    // call the function when the PickUp field changes\r\n    fd.field('PickUp').$on('change', toggleArrivalInfo);\r\n});",
    "css": "/* setting the Heading background depending on the theme */\r\n.fd-form h1 > span,\r\n.fd-form .fd-heading{\r\n   background-color: var(--fd-primary-rgb) !important;\r\n}\r\n\r\n\r\n\r\n",
