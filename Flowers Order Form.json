  "js": "function loadData() {\r\n    return $.get('https://raw.githubusercontent.com/Plumsail/forms-templates/main/data/flowers.json');\r\n}\r\n\r\n// prices\r\nvar products = {};\r\nproducts['Tulip'] = 0.75;\r\nproducts['Daffodil'] = 1.23;\r\nproducts['Poppy'] = 0.45;\r\nproducts['Sunflower'] = 2.35;\r\nproducts['Bluebell'] = 1.75;\r\nproducts['Rose'] = 2.15;\r\nproducts['Snowdrop'] = 1.45;\r\nproducts['Cherry blossom'] = 3.15;\r\n\r\nfd.rendered(function() {\r\n    var dataTable = fd.control('DataTable1');\r\n    var totalField = fd.field('OrderTotal');\r\n    \r\n    // populate table on form load\r\n    loadData().then(function(data){\r\n        dataTable.value = JSON.parse(data)\r\n    })\r\n\r\n    // make Unit Price column read-only\r\n    const unitPriceColumn = dataTable.columns.find(c => c.field === 'UnitPrice');\r\n    unitPriceColumn.editable = () => false;\r\n    \r\n    // make LineTotal column read-only\r\n    const lineTotalColumn = dataTable.columns.find(c => c.field === 'LineTotal');\r\n    lineTotalColumn.editable = () => false;\r\n    \r\n    // make Total field read-only\r\n    totalField.disabled = true;\r\n    \r\n    dataTable.$on('change', function(value) {\r\n        var total = 0.0;\r\n\r\n        if (value) {\r\n            for (var i = 0; i < value.length; i++) {\r\n                if (!value[i].Product) {\r\n                    value[i].set('Product', 'Tulip');\r\n                }\r\n\r\n                if (!value[i].Quantity) {\r\n                    value[i].set('Quantity', 1);\r\n                }\r\nconsole.log(value[i])\r\n                // populate UnitPrice column\r\n                value[i].set('UnitPrice', products[value[i].Product] || 0);\r\n\r\n                // calculate total for the row\r\n                value[i].set('LineTotal', value[i].Quantity * value[i].UnitPrice || 0);\r\n\r\n                total += parseFloat(value[i].LineTotal);\r\n            }\r\n        }\r\n\r\n        totalField.value = total;\r\n    });\r\n});\r\n",
  "css": ".fd-form  {\n    box-sizing: border-box;\n    background: url(https://raw.githubusercontent.com/Plumsail/forms-templates/main/images/b31a88d6-73d4-4155-b256-41b041401eab_bg.jpg) no-repeat center center fixed;\n    -webkit-background-size: auto;\n    -moz-background-size: cover;\n    -o-background-size: cover;\n    background-size: cover;\n}\n\n.fd-form.k-animation-container {\n    background-image: none !important;\n}\n\n.fd-form h1, .fd-form h4, .fd-form p {\n    color: #ffff !important;\n}\n\n.fd-form h1{\n    font-size: min(6.5vw, 2.34375rem) !important;\n    text-align:center !important\n}\n\n.fd-form h4, .fd-form .h4 {\n    font-size: min(4.5vw, 1.40625rem) !important;\n    padding-top: 4px !important;\n    padding-bottom: 4px !important;\n    margin-bottom: 1rem !important;\n    display: flex !important;\n    flex-direction: row !important;\n    color: white !important;\n}\n\n.fd-form h4:before, .fd-form h4:after, .fd-form h1:before, .fd-form h1:after{\n    content: '';\n    flex: 1 1;\n    border-bottom: 1px solid;\n    margin: auto;\n}\n\n.fd-form h4:before {\n  margin-right: 10px\n}\n\n.fd-form h4:after {\n  margin-left: 10px\n}\n\n.fd-form .success-animation {\n    padding: 20px;\n    background: rgba(0, 0, 0, 0.5);\n    backdrop-filter: blur(8px);\n    border-radius: 2px;\n}\n\n.fd-form .fd-blur-background {\n    background: rgba(0, 0, 0, 0.4);\n    backdrop-filter: blur(6px);\n    border-radius: 2px;\n}\n\n.fd-form .fd-total-field .k-select {\n    display: none !important;\n}",
