    "js": "fd.themeChanged = theme => {\r\n    $('.fd-form').attr('theme', theme);\r\n};\r\n\r\nfd.rendered(function() {\r\n    //disable fields\r\n    fd.field('Date').disabled = true;\r\n\r\n    //set Date to the current date\r\n    fd.field('Date').value = new Date();\r\n\r\n\r\n    //load the Moment.js library to calculate patient's age and disable dates\r\n    $.getScript('https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.24.0/moment-with-locales.min.js')\r\n        .then(function() {\r\n            calendarSetUp();\r\n\r\n        });\r\n\r\n    // add validation for Consent\r\n    fd.field('Consent').addValidator({\r\n        name: 'ConsentValidator',\r\n        error: 'You must confirm all points. If you still have questions, contact your family doctor.',\r\n        validate: function(value) {\r\n            if (fd.field('Consent').value.length < 6)\r\n                return false;\r\n\r\n            return true;\r\n        }\r\n    });\r\n\r\n    //call functions when changing field values\r\n    fd.field('Allergic').$on('change', showHideAllergicList);\r\n    fd.field('VaccineReaction').$on('change', showHideVaccineReactionList);\r\n    fd.field('СhronicСonditions').$on('change', showHideСhronicСonditionList);\r\n\r\n    //сalling functions on form loading\r\n    showHideAllergicList();\r\n    showHideVaccineReactionList();\r\n    showHideСhronicСonditionList();\r\n\r\n});\r\n\r\n\r\n//show/hide AllergicDetails list\r\nfunction showHideAllergicList() {\r\n    if (fd.field('Allergic').value == 'Yes') {\r\n        fd.field('AllergicDetails').hidden = false;\r\n    } else {\r\n        fd.field('AllergicDetails').hidden = true;\r\n    }\r\n}\r\n\r\n//show/hide Vaccine Reaction list\r\nfunction showHideVaccineReactionList() {\r\n    if (fd.field('VaccineReaction').value == 'Yes') {\r\n        fd.field('VaccineDetails').hidden = false;\r\n    } else {\r\n        fd.field('VaccineDetails').hidden = true;\r\n    }\r\n}\r\n\r\n//show/hide Сhronic Сonditions list\r\nfunction showHideСhronicСonditionList() {\r\n    if (fd.field('СhronicСonditions').value == 'Yes') {\r\n        fd.field('СhronicСonditionsDetails').hidden = false;\r\n    } else {\r\n        fd.field('СhronicСonditionsDetails').hidden = true;\r\n    }\r\n}\r\n\r\n//disable dates to allow only 18+ users\r\n//set up calendar start view\r\nfunction calendarSetUp() {\r\n    var today = moment(new Date());\r\n    var newDate = today.subtract(18, 'years');\r\n    fd.field('BirthDate').widgetOptions = {\r\n        max: moment(newDate).toDate(),\r\n        start: 'decade'\r\n    }\r\n}",
    "css": ".fd-form h1 {\n    font-size: min(6.5vw, 2.34375rem) !important;\n    color: white !important;\n}\n\n.fd-form h1 > span{\n   background-color: var(--fd-primary-rgb) !important;;\n}\n\nh3 > .fd-heading__heading-text {\n    background-color: var(--fd-light-rgb) !important;\n}\n\n.fd-form .k-calendar .k-header {\n    color: #868e96\n}\n\n.fd-form .header p {\n    font-size: 16px !important;\n}\n\n.fd-form .completion-date .form-control:disabled,\n.fd-form .completion-date .form-control[readonly],\n.fd-form .completion-date .k-datepicker .k-picker-wrap,\n.fd-form .completion-date  .form-control{\n    background-color: transparent;\n    border-style: none;\n    font-weight: bold;\n}\n\n.fd-form .completion-date .k-select {\n    display: none !important;\n}\n\n.fd-form .completion-date input {\n    padding-left: 0px;\n}\n\n.fd-form .btn-primary {\n    font-weight: 600 !important\n}",
